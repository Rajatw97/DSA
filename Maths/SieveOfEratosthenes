void SieveOfEratosthenes(int n) 
{ 
	
	boolean[] prime=new boolean[n+1]; 
	Arrays.fill(primes,true);
  
	for (int p=2; p*p<=n; p++) 
	{ 
		if (prime[p] == true) 
		{ 
			for (int i=p*p; i<=n; i += p) 
				prime[i] = false; 
		} 
	} 
	for (int p=2; p<=n; p++) 
	  if (prime[p]) 
		  System.out.println(p);
} 
